	display: inline;
	width: auto;
	height: auto;
	line-height: normal;
	vertical-align: 7%;
	background-image: none;
	background-position: 0% 0%;
	background-repeat: repeat;
	margin-top: 0;
	content: "+";
	font-family: Arial, 'helvetica neue', Helvetica, sans-serif !important;
}

